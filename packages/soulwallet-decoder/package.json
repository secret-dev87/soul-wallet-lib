{
    "name": "@soulwallet/decoder",
    "version": "0.0.10",
    "description": "> TODO: description",
    "author": "Jayden <z.cejay@gmail.com>",
    "homepage": "https://github.com/proofofsoulprotocol/soulwalletlib#readme",
    "license": "ISC",
    "main": "./lib.cjs/main.js",
    "module": "./lib.esm/main.js",
    "types": "./lib.esm/main.d.ts",
    "directories": {
        "lib": "lib.cjs",
        "test": "__tests__"
    },
    "files": [
        "lib.cjs",
        "lib.esm"
    ],
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/proofofsoulprotocol/soulwalletlib.git"
    },
    "scripts": {
        "typedoc": "typedoc",
        "docs": "npx typedoc --plugin typedoc-plugin-markdown typedoc-plugin-missing-exports src/main.ts",
        "tsc": "tsc",
        "build": "tsc --project ./tsconfig.esm.json;tsc --project ./tsconfig.cjs.json;ts-node ./create-package-json.ts",
        "javascript-obfuscator": "javascript-obfuscator",
        "release": "tsc && javascript-obfuscator --config ../../obfuscator.json ./lib --output ./lib",
        "generateBytes4": "ts-node --project ./tsconfig.dev.json ./src/dev/generateBytes4.ts"
    },
    "bugs": {
        "url": "https://github.com/proofofsoulprotocol/soulwalletlib/issues"
    },
    "dependencies": {
        "@soulwallet/abi": "workspace:*",
        "@soulwallet/result": "workspace:*",
        "@soulwallet/assets": "workspace:*",
        "ethers": "^6.7.0"
    },
    "devDependencies": {
        "@types/node": "^20.3.1",
        "@types/shelljs": "^0.8.12",
        "shelljs": "^0.8.5"
    },
    "exports": {
        ".": {
            "import": "./lib.esm/main.js",
            "require": "./lib.cjs/main.js"
        }
    }
}