{
  "name": "@soulwallet/soulwallet",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "tsc": "tsc",
    "javascript-obfuscator":"javascript-obfuscator",
    "ts-node": "ts-node",
    "preinstall": "npx only-allow pnpm",
    "build": "pnpm -r run tsc",
    "rebuild": "rm -rf ./packages/*/lib;pnpm -r run tsc",
    "release": "pnpm -r run release",
    "preTest": "cd soul-wallet-contract/ && chmod +x .env && npm install && npm run build && cd ../bundler/ && yarn && yarn preprocess",
    "test": "pnpm -r run test --filter 'test'",
    "generateABI": "pnpm -r run generateABI --filter 'soulwallet-abi'",
    "clean": "lerna run clean && rm -rf ./packages/*/lib/*",
    "reBuild": "npm run clean && npm run build  ",
    "geth": "docker-compose -f ./eth-node/docker-compose.yaml up --force-recreate -V",
    "bundlerInit": "cd bundler/ && yarn && yarn preprocess",
    "bundler": "cd bundler/ && yarn run bundler"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/node": "^20.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "javascript-obfuscator": "^4.0.2"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": ""
}